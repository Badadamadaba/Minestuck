{
    "multipart": [
        {
            "when": { "part": "totem_corner", "facing": "south" },
            "apply": { "model": "minestuck:alchemiter_totem_corner" }
        },
        {
            "when": { "part": "totem_corner", "facing": "west" },
            "apply": { "model": "minestuck:alchemiter_totem_corner", "y": 90 }
        },
        {
            "when": { "part": "totem_corner", "facing": "north" },
            "apply": { "model": "minestuck:alchemiter_totem_corner", "y": 180 }
        },
        {
            "when": { "part": "totem_corner", "facing": "east" },
            "apply": { "model": "minestuck:alchemiter_totem_corner", "y": 270 }
        },
        {
            "when": { "part": "totem_pad", "facing": "south" },
            "apply": { "model": "minestuck:alchemiter_totem_pad" }
        },
        {
            "when": { "part": "totem_pad", "facing": "west" },
            "apply": { "model": "minestuck:alchemiter_totem_pad", "y": 90 }
        },
        {
            "when": { "part": "totem_pad", "facing": "north" },
            "apply": { "model": "minestuck:alchemiter_totem_pad", "y": 180 }
        },
        {
